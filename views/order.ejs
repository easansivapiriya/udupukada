        animation: 20s slider infinite;
    }

    #slider figure img
    {
        width: 20%;
        float: left;
        height: 400px;
        padding: 10px;

    }

    @keyframes slider
    {
        0%{
            left: 0;
        }
        20%{
            left: 0;
        }
        25%{
            left: -100%;
        }
        45%{
            left: -100%;
        }
        50%{
            left: -200%;
        }
        70%{
            left: -200%;
        }
        95%{
            left: -300%;
        }
        100%{
            left: -400%;
        }
    }

